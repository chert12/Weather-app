<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:customcontrols="clr-namespace:Xamarin.CustomControls"
             xmlns:forms="clr-namespace:dotMorten.Xamarin.Forms;assembly=dotMorten.Xamarin.Forms.AutoSuggestBox"
             x:Class="xamarin_demo.Pages.AddCityModal">
    <ContentPage.Content>
        <StackLayout>
            <forms:AutoSuggestBox PlaceholderText="Enter city" WidthRequest="200" x:Name="entry"
                TextChanged="AutoSuggestBox_TextChanged"
                QuerySubmitted="AutoSuggestBox_QuerySubmitted"
                SuggestionChosen="AutoSuggestBox_SuggestionChosen"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>